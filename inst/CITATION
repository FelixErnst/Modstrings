year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)
bibentry(bibtype = "Manual",
         title = "{Modstrings}: implementation of Biostrings to work with nucleotide sequences containing modified nucleotides",
         author = c(person("Felix G.M.", "Ernst"),
                    person("Denis L.J.", "Lafontaine")),
         year = year,
         note = note)
